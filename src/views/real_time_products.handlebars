<div>
    <div>
        <h1>Productos en Vivo</h1>
    </div>

    <div>
        <ul id="updatedProductList">
            {{#each product}}
                <li>
                    <p>Title: {{title}}</p>
                    <p>Price: ${{price}}</p>
                    <p>Description: {{description}}</p>
                    <p>Stock: {{stock}}</p>
                </li>
                {{/each}}
        </ul>
    </div>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>